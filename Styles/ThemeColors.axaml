<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <ResourceDictionary.ThemeDictionaries>
        <ResourceDictionary x:Key='Light'>
            <Color x:Key='DarkBrush'>#EBEBEB</Color>
            <Color x:Key='MediumBrush'>#F4F4F4</Color>
            <Color x:Key='LightBrush'>#FFFFFF</Color>
            <Color x:Key='MenuBorderBrush'>#CCCCCC</Color>
            <Color x:Key='InputGestureBrush'>#808080</Color>
            <Color x:Key='ButtonBorderBrush'>#CCCCCC</Color>
            <Color x:Key='TitleBar'>#F4F4F4</Color>
            <Color x:Key='TextBrush'>#222222</Color>
        </ResourceDictionary>
        <ResourceDictionary x:Key='Dark'>
            <Color x:Key='DarkBrush'>#1A1A1A</Color>
            <Color x:Key='MediumBrush'>#202020</Color>
            <Color x:Key='LightBrush'>#2B2B2B</Color>
            <Color x:Key='MenuBorderBrush'>#454545</Color>
            <Color x:Key='InputGestureBrush'>#808080</Color>
            <Color x:Key='ButtonBorderBrush'>#000000</Color>
            <Color x:Key='TitleBar'>#3C3F41</Color>
            <Color x:Key='TextBrush'>#DFDFDF</Color>
        </ResourceDictionary>
    </ResourceDictionary.ThemeDictionaries>
</ResourceDictionary>